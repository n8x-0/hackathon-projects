{
    "public": "src/public",
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/view-resume",
        "destination": "/share/view-resume.html"
      }
    ],
    "builds": [
      {
        "src": "src/**/*.html",
        "use": "@vercel/static"
      },
      {
        "src": "src/**/*.css",
        "use": "@vercel/static"
      },
      {
        "src": "src/**/*.js",
        "use": "@vercel/static"
      }
    ]
  }
  